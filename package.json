{
  "name": "venus",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node scripts/dev.mjs && pnpm dev:before",
    "dev:before": "tauri dev",
    "build": "node scripts/build.mjs && node scripts/dev.mjs && pnpm build:before",
    "build:before": "tauri build",
    "dev:web": "next dev -p 1420",
    "build:web": "next build",
    "tauri": "tauri",
    "pretty-check": "prettier \"./**/*.{js,jsx,ts,tsx,json,md,mdx,css}\" --ignore-unknown --check",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,md,mdx,css}\" --ignore-unknown --config ./.prettierrc.json",
    "test": "vitest",
    "lint": "next lint",
    "prepare": "husky install",
    "fmt": "cd src-tauri/ && cargo clippy --fix --allow-dirty --allow-staged --all-features && cargo fmt",
    "fmt-check": "cd src-tauri/ && cargo fmt --check"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@monaco-editor/react": "^4.6.0",
    "@tauri-apps/api": "^1.5.6",
    "ahooks": "^3.7.11",
    "antd": "^5.17.3",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "modern-normalize": "^2.0.0",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-resizable": "^3.0.5",
    "tauri-plugin-autostart-api": "github:tauri-apps/tauri-plugin-autostart#v1",
    "use-immer": "^0.9.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "@next/bundle-analyzer": "^14.2.3",
    "vite-tsconfig-paths": "^4.3.2",
    "terminal-kit": "^3.1.1",
    "execa": "^9.1.0",
    "@tauri-apps/cli": "^1.5.14",
    "@testing-library/react": "^15.0.7",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-resizable": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "jsdom": "^24.0.0",
    "monaco-editor": "^0.48.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
